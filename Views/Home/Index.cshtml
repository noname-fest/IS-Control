@{
    ViewData["Title"] = "Home Page";
}

    <div class="text-center">
        <h1 class="display-4">Пользователь : @ViewData["UserFullName"]</h1>
        <div>Логин в системе : @ViewData["UserName"]</div>
        <div>Код ПП : @ViewData["UnitsId"]</div>
        <div>Доступ : @ViewData["Role"]</div>
        <p></p>
            @if (@ViewData["Role"].ToString().Trim() == "Admin")
            {
                <a asp-action="UsersEdit" asp-controller="Account">Список пользователей</a>
            }
        <p></p>

        <p></p>
        <form asp-action="Logout" asp-controller="Account">
            <input type="submit" value="Выйти из системы">
        </form>
    </div>
